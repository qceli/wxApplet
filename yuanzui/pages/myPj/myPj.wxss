page{
  font-family: '微软雅黑';
  width: 100%;
  height: 100%;
  background: #fff  
}
.scrollsBody{
  width: 100%;
  height: 100%;
  float: left;
  box-sizing: border-box;
  position: relative;
  font-family: '微软雅黑';
  padding: 0 2%
}
.nomore{
  width: 100%;
  float: left;
  text-align: center;
  font-size: 28rpx;
  box-sizing: border-box;
  padding: 20rpx 0;
  color: #666
}
.cardFade{
  width: 100%; 
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  background: rgba(0,0,0,0.3)
}
.cardLight{
  width: 86%;
  height: 880rpx;
  position: fixed;
  top: 7%;
  left: 7%;
  z-index: 101;
  background: #fff;
  border-radius:10px;
  box-shadow: 1px 2px 4px rgba(0,0,0,0.2);
  overflow: hidden   
}

.banner{
  width: 100%;
  float: left;
  height: 380rpx;
}
.bn{
  width: 100%;
  height: 100%
}
.bnImg{
  display: inline-block;
  width: 100%;
  height: 100%;
}
.search{
  width: 100%;
  background: #38BEB3; 
}
.tabs{
  width: 100%;
  float: left;
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-wrap: nowrap;
  background: #fff;
}
.tabs text{
  flex-grow:1;
  text-align: center;
  color: #666;
  font-size: 28rpx;
  border-bottom:solid 2px #fff;
  box-sizing: border-box;
  padding: 10rpx 0;
}
.tabactive{
  color: #f76260 !important;
  border-bottom: solid 2px #f76260 !important;
}
.classes{
  width: 100%;
  float: left;
  height: 93%;
}
.csUl{
  width: 100%;
  height: auto;
  float: left;
}
.csLi{
  width: 100%;
  float: left;
  height: auto;
  box-sizing: border-box;
  background: #fff;
  border:solid 1px #e5e5e5;
  margin-bottom: 20rpx;
  margin-top: 15rpx;
  background: #f6f6f6
}
.csLi .cl1{
  width: 100%;
  float: left;
  box-sizing: border-box;
  padding: 10rpx 3%;
  font-size: 35rpx;
}
.csLi .cl1 text{
  float: left;
  color: #555;
  font-size: 22rpx;
  margin-right: 1%
}
.csLi .cl1 text:last-child{
  float: right !important;
  color: #f60 !important;
}
.cl2{
  width: 100%;
  float: left;
  box-sizing: border-box;
  padding: 15rpx 3%;
}
.cl2 image{
  width: 150rpx;
  height: 150rpx;
  float: left;
  margin-right: 2%;
}
.cl3{
  width: 70%;
  float: left;
  font-size: 28rpx;
  color: #333;
  font-weight: bold;
}
.cl4{
  width: 70%;
  float: left;
  font-size: 26rpx;
  color: #555;
  margin-top: 20rpx;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;   
}
.cl5{
  width: 100%;
  float: left;
  box-sizing: border-box;
  padding: 10rpx 3%;
  display: flex;
  align-items: center;
  flex-wrap: nowrap; 
  justify-content: space-between
}
.cl6{
  float: left;
  color: #555;
  font-size: 26rpx;
}
.cl6 text{
  color: #f60
}
.cl7{
  width: 60%;
  float: right
}
.clbtn{
  width: 100rpx;
  border-radius:2px;
  color: #fff;
  font-size: 22rpx !important;
  padding: 6rpx 11rpx;
  margin-left: 3%;
  float: right;
  white-space: nowrap;
  text-align: center
}
.cl7 text:last-child{
  background: #ccc
}
.cl7 text:first-child{
  background: #222
}

.clist{
  width: 100%;
  float: left;
  box-sizing: border-box;
  padding: 0 3%;
}
.clist .cn{
  width: 100%;
  float: left;
  font-size: 28rpx;
  display: flex;
  justify-content: space-between;
  margin-top: 6rpx;
}
.clist .cn text:first-child{
  color: #666;
}
.clist .cn text:last-child{
  color: #111;
}
.pj{
  width: 100%;
  float: left;
  box-sizing: border-box;
  padding: 10rpx 3%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: nowrap;
  margin-bottom: 15rpx;
}
.pj text{
  font-size: 28rpx;
  color: #666;
}
.stars{
  width: 75%;
}
.star{
  width: 50rpx;
  height: 50rpx;
  float: left;
  margin-left: 20rpx;
}
.tags{
  width: 100%;
  float: left;
}
.tag{
  width: auto;
  float: left;
  background: #ddd;
  color: #666;
  box-sizing: border-box;
  padding: 10rpx 15rpx;
  border-radius:2px;
  font-size: 24rpx; 
  margin-right: 20rpx;
  margin-bottom: 20rpx;
}
.pt{
  width: 100%;
  float: left;
  height: auto;  
}
.pt1{
  width: 100%;
  float: left;
  font-size: 30rpx;
  margin-bottom: 20rpx;
}
.ptTxt{
  width: 100%;
  height: 320rpx;
  float: left;
  border: solid 1px #e5e5e5;
  box-sizing: border-box;
  padding: 20rpx;
  font-size: 28rpx;
  color: #333;
  padding-bottom: 50rpx;
}
.pt2{
  width: 100%;
  float: left;
  text-align: right;
  margin-top: -50rpx;
  color: #666;
  font-size: 26rpx;
  box-sizing: border-box;
  padding: 0 2%;
}
.fbBtn{
  width:100%;
  float:left;
  background:#444;
  color:#fff;
  box-sizing:border-box;
  padding:16rpx 0;
  border-radius:2px;
  font-size:28rpx;
  margin-top:40rpx;
  text-align: center
}
.tagactive{
  background: #444 !important;
  color: #fff !important;
}