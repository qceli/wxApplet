page{
  font-family: '微软雅黑';
  width: 100%;
  height: 100%;
  background: #fff  
}
.scrollsBody{
  width: 100%;
  height: 100%;
  float: left;
  box-sizing: border-box;
  position: relative;
  font-family: '微软雅黑';
  padding: 0 2%
}
.wl{
  width: 100%;
  float: left;
  margin-top: 30rpx;
}
.wl1{
  width: 80rpx;
  height: 80rpx;
  float: left;
  margin-right: 2%;
  margin-bottom: 50rpx;
  margin-top: 10rpx;
  margin-left: 4%
}
.wl2{
  width: 70%;
  float: left;
  font-size: 30rpx;
  color: #111;
}
.wl3{
  width: 70%;
  float: left;
  font-size: 26rpx;
  color: #666;
  margin-top: 5rpx;
}
.wl3 text{
  color: #0099CC;
}
.wltit{
  width: 100%;
  border-bottom:solid 1px #e5e5e5;
  float: left;
  font-size: 26rpx;
  color: #666;
  margin-top: 50rpx; 
  box-sizing: border-box;
  padding: 15rpx 4%;
  margin-bottom: 20rpx;
}
.wlUl{
  width: 100%;
  float: left;
  box-sizing: border-box;
  padding: 20rpx 4%;
}
.wlli{
  width: 100%;
  float: left;
  box-sizing: border-box;
  border-left:solid 2px #ccc; 
  padding: 0 4%;
  position: relative;
}
.w1{
  width: 100%;
  float: left;
  font-size: 28rpx;
  color: #666;
  margin-top:-12rpx;
}
.w2{
  width: 100%;
  float: left;
  font-size: 24rpx;
  color: #666;
  margin-top: 5rpx;
  margin-bottom:70rpx;
}
.w3{
  position: absolute;
  z-index: 5;
  top: 0;
  left: -12rpx;
  width: 20rpx;
  height: 20rpx;
  border-radius:50%;
  background: #ccc;
}
.wlUl .wlli:first-child .w1{
  color: #333 !important;
}
.wlUl .wlli:first-child .w2{
  color: #333 !important; 
}
.wlUl .wlli:first-child .w3{
  background: #333;
  top: 0;
  left: -17rpx;
  width: 30rpx;
  height: 30rpx;  
}
