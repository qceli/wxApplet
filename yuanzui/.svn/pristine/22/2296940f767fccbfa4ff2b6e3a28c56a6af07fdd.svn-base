page{
  font-family: '微软雅黑';
  width: 100%;
  height: 100%;
  background: #fff  
}
.scrollsBody{
  width: 100%;
  height: 100%;
  float: left;
  box-sizing: border-box;
  position: relative;
  font-family: '微软雅黑';
  padding: 2% 3%;
}
.mcType{
  width: 100%;
  float: left;
  border-bottom:solid 1px #e5e5e5;
  box-sizing: border-box;
  margin-bottom: 20rpx;
}
.tab{
  width: auto;
  float: left;
  box-sizing: border-box;
  padding: 8rpx 0;
  color: #666;
  border-bottom:solid 2px #fff;
  font-size: 30rpx;
  margin-right: 40rpx
}
.tabactive{
  color: #222;
  border-bottom:solid 2px #444;
  font-weight: bold  
}
.classUl{
  width: 100%;
  float: left;
  height: 93%;
}
.classLi{
  width: 100%;
  float: left;
  background: #f2f2f2;
  box-sizing: border-box;
  padding: 20rpx;
  border-radius:4px;
  margin-bottom: 30rpx;
}
.cl1{
  width: 200rpx;
  height: 200rpx;
  float: left;
  border-radius:4px;
  margin-right: 20rpx; 
}
.cl2{
  width: 64%;
  float: left;
  font-size: 30rpx;
  font-weight: bold;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.cl3{
  width: 64%;
  float: left;
  font-size: 28rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #666;
  margin-top: 14rpx;  
}
.cl4{
  box-sizing: border-box;
  padding: 10rpx 15rpx;
  background: #333;
  color: #fff;
  font-size: 22rpx;
  float: left;
  border-radius:4px;
  margin-top: 40rpx;
}
.cl5{
  width: 64%;
  float: right;
  font-size: 24rpx;
  color: #666;
  text-align: right;
  margin-top: -70rpx
}
.cl6{
  width: 64%;
  float: right;
  font-size: 24rpx;
  color: #666;
  text-align: right;
  margin-top: -32rpx  
}
