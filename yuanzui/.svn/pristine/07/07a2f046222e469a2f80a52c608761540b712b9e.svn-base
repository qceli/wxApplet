/* pages/unpaidOrderInfo/unpaidOrderInfo.wxss */
.shippingAddress {
  margin: 40rpx 0 0 0;
  display: flex;
  color: #999;
  font-size: 32rpx;
}
.telImg  {
  width: 50rpx;
  height: 75rpx;
  margin: 25rpx 10rpx 0 0;
}
.telImg>image {
  width: 50rpx;
  height: 50rpx;
}
.addressInfo {
  width: calc(100% - 280rpx);
}
.addressInfo>view {
  line-height: 50rpx;
}
.orderPlace {
  font-size:30rpx;
}
.telNum {
  margin-left: 10rpx;
  line-height: 100rpx;
  width: 210rpx;
}
.lineImg, .lineImg>image {
  width: 100%;
  margin: 0;
  padding: 0;
}
.lineImg>image {
  width: 100%;
  height: 20rpx;
}
.jewelryInfo {
  display: flex;
  margin-bottom: 20rpx;
}
.jewelryImg {
  width: 200rpx;
  height: 200rpx;
  margin-right: 20rpx;
}
.jewelryImg>image {
  width: 200rpx;
  height: 200rpx;
}
.jewelryName {
  font-size: 36rpx;
  height: 100rpx;
  line-height: 120rpx;
  font-weight: bold;
}
.jewelryContent {
  color: #999;
  font-size: 32rpx;
  line-height: 45rpx;

}
.orderInfo {
  /* display: flex; */
  clear: both;
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 36rpx;
}
.leftInfo {
  float: left;
  color: #999;
}
.rightInfo {
  float: right;
}
.orderPrice {
  clear: both;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: solid 1px #ccc;
  border-top: solid 1px #ccc;
  margin-top: 20rpx;
  font-size: 36rpx;
}

.blackBottom {
  text-align: center;
}