/* pages/business/business.wxss */
.top-img,.top-img>image {
  width: 100%;
  height:600rpx;
}
.middle {
  width:385rpx;
  height:100rpx;
  border-radius: 100rpx;
  background: #d0b763;
  margin-top: -50rpx;
  z-index:1;
  position:absolute;
  left:25%;
  text-align:center;
  line-height:90rpx;
  color:#fff;
  font-size:50rpx;
  font-weight:bold;
}
.middle>view {
  margin: 3rpx;
  width:375rpx;
  height:90rpx;
  border-radius: 90rpx;
  border:1px solid #fff;
}

.businesses {
  background-color: #fff9e1;
  padding-top: 35rpx;
  width: 100%;
}
.business-list {
  width: 100%;
  border-bottom:1px solid #ccc;
}
.business-img {
  margin-left: 30%;
}
.business>view {
  display: inline-block;
}
.business-img>image {
  margin:50rpx auto;
  width:100rpx;
  height:80rpx;
}
.business-name {
  line-height:180rpx;
  color: #666;
  padding-left:40rpx;
  float:right;
  margin-right:30%;
}