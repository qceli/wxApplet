/* pages/returnTicket/returnTicket.wxss */
.container {
  height: 100%;
  background-color: #efeef4;
}
.title, .money, .method {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 20rpx;
}
.money, .method {
  background-color: #fff;
}
.money>view, .method>view {
  float: left;
}
.number {
  color:#ff9900;
  font-weight:bold;
  margin-left: 40rpx;
  font-size:28rpx;
}
.methods {
  width:calc(100% - 120rpx);
}
.time {
  color: #ff6600;
  margin-left: 20rpx;
}
.img {
  width: 100rpx;
  height: 100rpx;
  /* margin-left:70rpx; */
}
.img>image {
  width:50rpx;
  height:50rpx;
  margin-top:25rpx;
}
.return {
  width:calc(100% - 160rpx);
  height:80rpx;
  line-height:80rpx;
  text-align:center;
  background-color:#ff6600;
  color:#fff;
  border-radius:5px;
  float:left;
  margin:100rpx 80rpx 0 80rpx;
  font-size:32rpx;
  font-weight:bold;

}