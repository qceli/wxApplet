/**index.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 100rpx 0;
  box-sizing: border-box;
} 

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}
.getinfo{
  width:85%;
  margin-top: 50px;
}
.wranginfo{
  padding:0rpx 52rpx;
  margin-top:40rpx;
}
.wranginfo span{
  display:block;
  font-size:13px;
  line-height:44rpx;
  margin-bottom:10rpx;
  color:#333;
}
.isHeight{
  margin-top:500rpx;
}
.wranginfo .wranginfo-title{
  text-align:center;
  font-size:15px;
}
