<!-- index.wxml -->
<scroll-view scroll-y="true" class="index-scroll" bindscrolltolower="lower" >
<map id="map" longitude="{{log}} " latitude="{{lat}}" scale="14" controls="{{controls}}" bindcontroltap="primary" markers="{{markers}}" bindmarkertap="markertap" polyline="{{polyline}}" bindregionchange="regionchange" show-location style="width: 100%; height: 100%;"></map>
</scroll-view>


