/* pages/minebike/minebike.wxss */
page {
  height: 100%;
}
.content {
  height: 100%;
  background-color: #eee;
}
.lists {
  /* padding-bottom: 1px; */
  /* height: calc(100% - 150rpx); */
  height: 100%;  
}
.list {
  padding: 40rpx 20rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
  display:flex;
  font-size: 30rpx;
  color: #666;
}
.listImg {
  width: 100rpx;
  height: 100rpx;
  border-radius: 5px;
  border: 1px solid #ccc;
}
.listImg>image {
  width: 100rpx;
  height: 100rpx;
}
.list-middle {
  padding-left: 40rpx;
  height: 50rpx;
  line-height: 50rpx;
}
.list-last {
  padding-left: 20rpx;
  height: 50rpx;
  line-height: 50rpx;  
  /* width:400rpx; */
}
.nodatas,.load-more {
  margin: 0; 
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  color: #ccc;
  font-size: 30rpx;
}
.bottom {
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 150rpx;
  background-color: #fff;
  display: flex;
}
.apply {
  flex: 1;
  margin: 35rpx 30rpx 0 30rpx;
  height: 80rpx;
  line-height: 80rpx;  
  border: 1px solid #ccc;
  border-radius: 5px;
  text-align: center;
  color: #fff;
  background-color: #f98b2a;
}