/**complete.wxss**/
page {
    font-size: 10px;
    padding: 0;
    margin: 0;
    width:100%;
    height:100%;
}
.bg {
  z-index: -1;
  position: fixed;
  width:100%;
  height:calc(100% - 1rpx);
}
.container {
  height:100%;
  display:flex;
  flex-direction:column;
  align-items:center;
  padding:200rpx 0;
  box-sizing:border-box;
  top:40px;
  position:fixed;
  width:100%;
} 
.icon-complet:before {
  content:"\e626";
  line-height:64px;
  font-size:80rpx;
  color:#2ba0ff;
  display:-webkit-flex;
  /*align-items:center;*/
  justify-content:center;
}

.userinfo-avatar {
  width:68px;
  height:68px;
  border-radius: 50%;
  overflow:hidden;
  z-index:1;
}
.userinfo {
  display: flex;
  flex-direction: column;
  /*align-items: center;*/
  border: 1px solid #d9d8d2;
  border-radius: 6px;
  background-color: #f4f4ef;
  width: 90%;
  height: 190px;
  margin-top:-34px;
  z-index: -1;
}
.info {
  border-bottom: 1px solid #d9d8d2;
  width: 90%;
  height:60px;
  font-size:30rpx;
  color: #696969;
  text-align: center; 
  margin: 0 auto;
}
.userinfo-name {
  /*height:calc(60px - 0.5rem);
  line-height:calc(90px - 1rem);*/
  height:30px;
  line-height:30px;
  margin-top:45px;
}
.infos {
  line-height:90px;
  color:#cccccc;
  height:30px;
}
.company {
  padding-top:30px;
}
.next {
  width: 60px;
  height: 60px;
  background-color: #f4f4ef;
  border: solid 2px rgba(191, 220, 222, 0.6);
  border-radius: 50%;
  -moz-border-radius: 50%;
  -webkit-border-radius: 50%;
  margin-top:-30px;
  z-index: 1;

}
.bottom {
  color:#ffffff;
  font-size:20rpx;
  position:fixed;
  bottom:16rpx;
  text-align:center;
  left:40%;
}
