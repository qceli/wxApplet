.search {
  padding:8px 40rpx;
  width:calc(100% - 80rpx);
  height:30px;
  background-color:#459de6;
}
.search-top {
  padding: 0 40rpx;
  width:calc(100% - 80rpx);
  height: 28px;
  border-radius: 14px;
  background-color: #f5f5f5;
  border: solid 0.5px #e6e6ea;
}
.search-form {
  width:100%;
}
.search-input {
  width: calc(100% - 1rem);
  font-size: 26rpx;
  float:right;
}
.icon-search:before {
  content: "\e627";
  font-size:20rpx;
}
.icon-recent:before {
  content: "\e603";
  font-size:54rpx;
}
.icon-all:before {
  content: "\e622";
  font-size:54rpx;
}
.content {
  padding: 40rpx 40rpx 0 40rpx;
  width: calc(100% - 80rpx);
  height: calc(100% - 230rpx);
  background-color: #f5f5f5;
}
.cadlists {
  width: calc(100% - 80rpx);
  height: 130rpx;
  border-radius: 3px;
  background-color: #ffffff;
  box-shadow: 0px 1.5px 3.5px 0 rgba(55, 62, 70, 0.1);
  padding: 40rpx;
  margin-bottom: 40rpx;

}
.listtop {
  height: 80rpx;
}
.headimg {
  float: left;
  height: 80rpx;
  width: 80rpx;
  border-radius: 50%;
  padding-right:1rem;
}
.headImg {
  height: 80rpx;
  width: 80rpx;
}
.listtop-right {
  float: left;
  height: 80rpx;
  padding:10rpx 40rpx 10rpx 0;
}
.cadfile {
  font-size: 28rpx;
  color: #798896;
}
.cadcode {
  padding-top: 10rpx;
  font-size: 22rpx;
  color: #b1c0ce;
}
.designer {
  padding-top: 30rpx;
  font-size: 28rpx;
  color: #798896;
}

.bottom {
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 98rpx;
  background-color: #fafbfc;
  box-shadow: 0px -1px 10px 0 rgba(55, 62, 70, 0.1);
  display: -webkit-flex;
}
.botm {
  text-align: center;
  flex: 1;
}
.recentlist {
  color: #b1c0ce;
}
.alllist {
  color: #459de6;
}